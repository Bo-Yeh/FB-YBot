# Copy to .env and fill values; DO NOT commit real secrets
API_KEY=your_openai_api_key
FB_TOKEN=your_facebook_access_token

IG_USERNAME=your_instagram_username
IG_PASSWORD=your_instagram_password
# Prefer using a verified sessionid when possible
IG_SESSIONID=
# Provide one of: IG_SETTINGS_JSON or IG_SETTINGS_PATH
IG_SETTINGS_JSON=
IG_SETTINGS_PATH=downloads/instagrapi_settings.json
# Optional proxy: http(s)://user:pass@host:port
IG_PROXY=

NEWS=https://www.setn.com/ViewAll.aspx
MODE=setn
POST_DELAY_MIN=1800
POST_DELAY_MAX=7200
POST_TO_FACEBOOK=true
POST_TO_INSTAGRAM=true
